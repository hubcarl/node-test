{
  "name": "egg-test",
  "version": "1.0.0",
  "description": "NPM package template",
  "keywords": [
    "npm",
    "npm package",
    "unit test",
    "eslint",
    "travis",
    "travis-ci",
    "code coverage",
    "changelog"
  ],
  "dependencies": {
    "egg": "^1.0.0",
    "egg-view-nunjucks": "^2.2.0",
    "humanize-ms": "^1.2.1"
  },
  "devDependencies": {
    "chai": "^4.1.1",
    "codecov": "^3.0.0",
    "conventional-changelog-cli": "^1.3.5",
    "cross-env": "^5.0.5",
    "egg-bin": "^4.15.0",
    "egg-mock": "^3.0.0",
    "eslint": "^4.5.0",
    "eslint-config-eslint": "^4.0.0",
    "mocha": "^3.5.0",
    "nyc": "^11.1.0"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "lint": "eslint .",
    "fix": "eslint --fix .",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "test": "egg-bin test",
    "cov": "nyc report --reporter=lcov && codecov --token=e971d1e7-39e2-44b1-b1ee-f4345b0adbee",
    "ci": "npm run lint && npm run cov",
    "ii": "npm install --registry https://registry.npm.taobao.org",
    "postinstall": "cp -r ./script/egg-cookies/lib/ ./node_modules/egg-cookies/lib"
  },
  "nyc": {
    "exclude": [
      "**/*.spec.js",
      "test/*.test.js"
    ]
  },
  "ci": {
    "version": "6, 8, 10, 12, 14"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hubcarl/npm-package-template.git"
  },
  "bugs": {
    "url": "https://github.com/hubcarl/npm-package-template/issues"
  },
  "homepage": "https://github.com/hubcarl/npm-package-template#readme",
  "author": "hubcarl@126.com",
  "license": "MIT",
  "webpack": {
    "loaders": {}
  }
}
